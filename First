#Create_List -

list=[]
for i in range(4):
    i=input("Enter Alphabet-")
    list.append(i)
print(list)

#Read_List -

list=["A","B","C","D"]
find=input("Enter Alphabet to Search-")
for i in range(len(list)):
    if find==(list[i]):
        print("Present")
        break
    elif i == (len(list)):
        print("Absent")
else:
    print("Absent")
 
#Update_List -

list=["A","B","C","D"]
list.append("E")
print(list)

#Delete_List

list=["A","B","C","D"]
del list
print(list)
